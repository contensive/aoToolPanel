		<ProcessInterval>0</ProcessInterval>
		<PageTitle></PageTitle>
		<RemoteAssetLink></RemoteAssetLink>
		<Styles></Styles>
		<Scripting Language="" EntryPoint="" Timeout="5000">
			<Code><![CDATA[function m
	dim name
	dim value
	'
	name = cp.doc.getProperty("n")
	value = cp.doc.getProperty("v")
	if (name="isLockedAccountTab") Then
		call cp.user.setProperty("isLockedAccountTab",value)
	end if
	if (name="tpLoginTabIsPinned") Then
		m = m & "<div>hit on tpLoginTabIsPinned</div>"
		call cp.user.setProperty("tpLoginTabIsPinned",value)
	end if
	m = m & "<div>n=[" & name & "]</div>"
	m = m & "<div>v=[" & value & "]</div>"
	m = m & "<div>property=[" & cp.user.getProperty("tpLoginTabIsPinned","") & "]</div>"
	cp.utils.appendLogfile( m )
end function]]></Code>
		</Scripting>
	</Addon>
